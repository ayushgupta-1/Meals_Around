{
  "expo": {
    "name": "Meals Around",
    "slug": "meals-around",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An application to browse some restaurants around you. Shows menu of restaurants and user's favourites restaurants. This application can be converted to large scale application by adding delivery and payments features.",
    "githubUrl": "https://github.com/ayushgupta-1/Meals_Around"
  }
}
